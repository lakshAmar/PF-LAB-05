#include <stdio.h>

int main() {
    int a, b, c, d;

    printf("Enter four sides: ");
    scanf("%d %d %d %d", &a, &b, &c, &d);

    if (a<=0 || b<=0 || c<=0 || d<=0) {
        printf("Not a valid quadrilateral");
    }
    else if (a==b && b==c && c==d) {
        printf("Square");
    }
    else if (a==c && b==d) {
        printf("Rectangle");
    }
    else {
        printf("General Quadrilateral");
    }

    return 0;
}
